    "AS4DATE":"0000-00-00",
    "AS4TIME":"00:00:00",
    "SOURCE":"@AbapCatalog.sqlViewName: 'ZI_CT_EXAMPLE'\r\n@AbapCatalog.compiler.compareFilter: true\r\n@AbapCatalog.preserveKey: true\r\n@AccessControl.authorizationCheck: #NOT_REQUIRED\r\n@EndUserText.label: 'DDL Source Contacts Example'\r\ndefine view ZI_CONTACTS_EXAMPLE\r\n  as select from    snwd_bpa_contact as CONT\r\n    left outer join snwd_bpa         as BP on BP.node_key = CONT.parent_key\r\n{\r\n  key BP.bp_id           as BusinessPartnerID,\r\n  key CONT.contact_id    as ContactID,\r\n      CONT.first_name    as FirstName,\r\n      CONT.last_name     as LastName,\r\n      @Semantics.eMail.type: [#WORK]\r\n      CONT.email_address as eMail,\r\n      @Semantics.telephone.type: [#WORK]\r\n      CONT.phone_number  as PhoneNumber,\r\n      @Semantics.telephone.type: [#CELL]\r\n      CONT.mobile_number as Mobile,\r\n      // a contact needs mandatorily a fullname\r\n      concat_with_space( CONT.first_name, CONT.last_name , 1 )\r\n                         as FullName,\r\n      CONT.language      as Language\r\n      \r\n}\r\n/*+[internal] {\n\"BASEINFO\":\n{\n\"FROM\":\n[\n\"SNWD_BPA\",\n\"SNWD_BPA_CONTACT\"\n],\n\"ASSOCIATED\":\n[],\n\"BASE\":\n[],\n\"ANNO_REF\":\n[],\n\"SCALAR_FUNCTION\":\n[],\n\"VERSION\":0,\n\"ANNOREF_EVALUATION_ERROR\":\"\"\n}\n}*/",
